{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "FichajeTray (m√≥dulo)",
        "type": "python",
        "request": "launch",
        "module": "src.main",
        "console": "integratedTerminal",
        "justMyCode": false,
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        },
        "python": "${workspaceFolder}\\.venv\\Scripts\\python.exe"
      },
      {
        "name": "Microsoft cloud sandbox",
        "request": "launch",
        "type": "al",
        "environmentType": "Sandbox",
